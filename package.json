{
  "name": "rocket-webpack",
  "version": "0.0.2",
  "description": "Simple webpack starter kit for fast starting projects",
  "repository": "https://github.com/mercury-oe/rocket-webpack.git",
  "author": "Oleg Ambrosevich <oleg.ambrosevich@yandex.ru>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn run dev",
    "dev": "node tools/scripts/start.js",
    "build": "node tools/scripts/build.js",
    "build:analyze": "yarn run build --analyze",
    "build:smp": "yarn run build --smp",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:write": "yarn run eslint && yarn run stylelint",
    "eslint": "eslint \"./src/**/*.js\"",
    "eslint:write": "eslint ./src/**/*.js --fix",
    "stylelint": "stylelint \"./src/**/*.css\"",
    "stylelint:write": "stylelint \"./src/**/*.css\" --fix"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/runtime": "7.3.4",
    "@svgr/webpack": "4.1.0",
    "autoprefixer": "9.5.0",
    "babel-loader": "8.0.5",
    "cache-loader": "2.0.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "clean-webpack-plugin": "2.0.1",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "eslint": "5.15.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "file-loader": "3.0.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "7.0.0",
    "imagemin-svgo": "7.0.0",
    "imagemin-webp-webpack-plugin": "3.1.0",
    "imagemin-webpack": "4.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.16.4",
    "react-dev-utils": "8.0.0",
    "speed-measure-webpack-plugin": "1.3.1",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-order": "2.1.0",
    "terser-webpack-plugin": "1.2.3",
    "thread-loader": "2.1.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5"
  }
}
