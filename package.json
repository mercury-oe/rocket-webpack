{
  "name": "webpack-starter-kit",
  "version": "0.0.1",
  "description": "Simple webpack starter kit for fast starting projects",
  "main": "index.js",
  "repository": "https://github.com/mercury-oe/webpack-starter-kit.git",
  "author": "Oleg Ambrosevich <oleg.ambrosevich@yandex.ru>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "yarn run dev",
    "dev": "webpack-dev-server --config tools/webpack.dev.js",
    "build": "webpack --config tools/webpack.prod.js --progress --hide-modules"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.1.5",
    "babel-loader": "8.0.4",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0"
  }
}
