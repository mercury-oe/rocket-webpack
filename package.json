    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "3.1.10",
